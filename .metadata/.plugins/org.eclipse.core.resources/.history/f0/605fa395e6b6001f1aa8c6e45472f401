package clraivate;

public class Cuestions {

	public static void main(String[] args) {

//		System.out.println(count("aaaaaabbbbbcccddd", 'd'));
		
		System.out.println(numCount());

	}

	public static int count(String s, char c) {

		int count = 0;

		for (int i = 0; i < s.length(); i++) {

			if (s.charAt(i) == c)
				count++;
			
			
		}

		return count++;

	}
	
	
	public static int numCount() {
		
		String s = "abcdree10sewe5hhsh4jdj3";
		
		int res = 0;
		
	        char[] ch = s.toCharArray();
	        
	        for(char c: ch){
	        	
	        	res = res+c;
	        	
	        	System.out.println(res);
	        	
	        	
	        	
	        }
			return res;
	        
	      
		
	}

}
