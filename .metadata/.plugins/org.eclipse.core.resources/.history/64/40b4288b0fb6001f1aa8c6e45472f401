package clraivate;

public class Cuestions {

	public static void main(String[] args) {

		System.out.println(count("aaaaaabbbbbcccddd", 'd'));

	}

	public static int count(String s, char c) {

		int count = 0;

		for (int i = 0; i < s.length(); i++) {

			if (s.charAt(i) == c)
				count++;
			
			
		}

		return count++;

	}
	
	
	public static void numCount() {
		
		String s = "abcdree10sewe5hhsh4jdj3";
		
	        char[] ch = s.toCharArray();
	        
	        for(char c: ch){
	        	
	        	System.out.println(c+c);
	        	
	        }
		
	}

}
