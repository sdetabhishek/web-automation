package launcher;

public class Arrays {

	public static void main(String[] args) {

		printArray();
		
	}
	
	
	static void printArray() {
		
		
		int arr[] = new int[] {10,20,30,40,50};
		
		System.out.println(arr.length);
		
		int i=0;
		
		while(i<=arr.length) {
			
			System.out.println(arr[i]);
			i++;
		}
		
		
	}

}
